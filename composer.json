{
  "name": "mosparo/mautic-integration-bundle",
  "description": "This plugin adds the required form field to Mautic to protect your forms with mosparo.",
  "keywords": ["mosparo", "mautic", "integration", "plugin", "bundle"],
  "type": "mautic-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "mosparo",
      "homepage": "https://mosparo.io"
    }
  ],
  "require": {
    "php": ">=7.4.0 <8.1",
    "mautic/core-lib": "^5.0"
    "mosparo/php-api-client": "^1.1"
  },
  "replace": {
    "guzzlehttp/guzzle": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "MauticPlugin\\MosparoIntegrationBundle\\": "."
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MauticPlugin\\MosparoIntegrationBundle\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "install-directory-name": "MosparoIntegrationBundle",
    "display-name": "mosparo Integration Bundle"
  }
}
